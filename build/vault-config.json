{
    "backend": {
        "file": {
            "path": "/vault/file"
        }
    },
    "default_lease_ttl": "168h",
    "listener": {
        "tcp": {
            "address": "0.0.0.0:8200",
            "tls_cert_file": "/vault/tls/cert.pem",
            "tls_disable": "false",
            "tls_key_file": "/vault/tls/cert-key.pem"
        }
    },
    "max_lease_ttl": "720h",
    "telemetry": {}
}
